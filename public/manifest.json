{
  "manifest_version": 2,

  "name": "My Extension",
  "description": "This extension is a starting point to create a real Chrome extension",
  "version": "0.0.1",

  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "icons": {
    "128" : "logo192.png"
    },
  "permissions": ["tabs"],
  "content_security_policy": "script-src 'self' 'sha256-T4inb6g26dN+sMoboDWzrmo8pqcmbLryeyyrdp1qtMo='; object-src 'self'",
  "chrome_url_overrides" : {
    "newtab": "index.html"
  }
}